#include <iostream>
using namespace std;

int main (){
    double num1, num2, num3;

    cout << "Input number 1: ";
    cin >> num1;
    cout << "Input number 2: ";
    cin >> num2;
    cout << "Input number 3: ";
    cin >> num3;

    double largest;

    if (num1 >= num2 && num1 >= num3) {
        largest = num1;
    } else if (num2 >= num1 && num2 >= num3){
        largest = num2;
    } else {
        largest = num3;
    }

    cout << "The largest number is: " << largest << endl;
    return 0;
}